{
  "name": "reactive-grid",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild:client": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack",
    "build:server": "tsc server.ts",
    "start:server": "nodemon",
    "start:client": "webpack -w",
    "start:server-prod": "node ./dist/server.js",
    "start": "yarn run start:client & yarn run start:server",
    "postinstall": "yarn run build:server && yarn run build:client",
    "get-schema":
      "apollo-codegen introspect-schema https://api.github.com/graphql --output schema.json --header \"Authorization: Bearer 1e37b0e75fea9218be6fbff4cf46db989065062f\"",
    "generate-types":
      "apollo-codegen generate **/*.tsx --schema schema.json --target typescript --output schema.ts",
    "relay":
      "relay-compiler --src ./src --schema ./schema.graphql --extensions tsx"
  },
  "author": "Vitalij Kudresov",
  "license": "ISC",
